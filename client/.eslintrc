{
   "extends": ["react-app", "airbnb", "prettier", "prettier/react"],
   "plugins": ["react", "prettier"],
   "env": {
      "browser": true,
      "node": true
   },
   "rules": {
      "prettier/prettier": "error",
      "react/jsx-filename-extension": ["off"],
      "react/forbid-prop-types": ["off"]
   }
}
